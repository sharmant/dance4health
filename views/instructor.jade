      function xy(x) {
      o = document.getElementById(x);
      l =o.offsetLeft;
      t = o.offsetTop;
      while (o=o.offsetParent) {
      l += o.offsetLeft;
      t += o.offsetTop;
      }
      t=t+document.body.offsetTop;
      l=l+document.body.offsetLeft;
      return { top: t, left: l };
